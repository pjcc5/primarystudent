<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="cn.pjc.mapper.AcountMapper">
  		<insert id="saveAcount" parameterType="Acount">
  			insert into acount(aid,aname,apass,aphone,amail,asex,alevel,arole,acontent,acreatetime,alastchangetime,athumbsup,aidcardnumber) 
  			values (#{aid},#{aname},#{apass},#{aphone},#{amail},#{asex},#{alevel},#{arole},#{acontent},#{acreatetime},#{alastchangetime},#{athumbsup},#{aidcardnumber})
  		</insert>
  		
  </mapper>